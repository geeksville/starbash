    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: starbash user",
            "type": "debugpy",
            "request": "launch",
            "module": "starbash.main",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.vscode/get_poetry_python.sh",
            "justMyCode": false,
            "args": [
                "--debug",
                "user",
                "email",
                "kevinh@geeksville.com"
            ]
        },
        {
            "name": "Python Debugger: starbash select",
            "type": "debugpy",
            "request": "launch",
            "module": "starbash.main",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.vscode/get_poetry_python.sh",
            "justMyCode": false,
            "args": [
                "--debug",
                "select",
                "list"
            ]
        },
        {
            "name": "Python Debugger: starbash process siril",
            "type": "debugpy",
            "request": "launch",
            "module": "starbash.main",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.vscode/get_poetry_python.sh",
            "justMyCode": false,
            "args": [
                "--debug",
                "process",
                "siril",
                "5",
                "/tmp/sir1"
            ]
        },
        {
            "name": "Python Debugger: starbash process masters",
            "type": "debugpy",
            "request": "launch",
            "module": "starbash.main",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.vscode/get_poetry_python.sh",
            "justMyCode": false,
            "args": [
                "--debug",
                "process",
                "masters"
            ]
        },
        {
            "name": "Python Debugger: starbash repo",
            "type": "debugpy",
            "request": "launch",
            "module": "starbash.main",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.vscode/get_poetry_python.sh",
            "justMyCode": false,
            "args": [
                "--debug",
                "repo",
                "add",
                "--master",
                "/tmp/repo/master"
            ]
        },
        {
            "name": "Python Debugger: processing script",
            "type": "debugpy",
            "request": "launch",
            "program": "poc/process.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.vscode/get_poetry_python.sh"
        },
        {
            "name": "Python Debugger: PyTest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.vscode/get_poetry_python.sh"
        }
    ]
}